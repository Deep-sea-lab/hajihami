{
  "version": 2,
  "functions": {
    "api/songs-vercel.js": {
      "maxDuration": 10,
      "memory": 512
    },
    "api/search-vercel.js": {
      "maxDuration": 10,
      "memory": 512
    },
    "api/ping-vercel.js": {
      "maxDuration": 10,
      "memory": 128
    },
    "api/favicon.js": {
      "maxDuration": 10,
      "memory": 128
    },
    "api/index.js": {
      "maxDuration": 10,
      "memory": 128
    }
  },
  "routes": [
    {
      "src": "/api/songs",
      "dest": "/api/songs-vercel.js",
      "methods": ["GET"]
    },
    {
      "src": "/api/search",
      "dest": "/api/search-vercel.js",
      "methods": ["GET"]
    },
    {
      "src": "/api/ping",
      "dest": "/api/ping-vercel.js",
      "methods": ["GET"]
    },
    {
      "src": "/favicon.ico",
      "dest": "/api/favicon.js"
    }
  ]
}
